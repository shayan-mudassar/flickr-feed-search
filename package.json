{
  "name": "flickr-photo-feed",
  "version": "0.0.1",
  "description": "Consuming Flickr photo feed using different kinds of front-end technologies",
  "main": "index.js",
  "scripts": {
    "react:dev": "webpack-dev-server ./react --config ./react/webpack.config.js --env.NODE_ENV=development",
    "react:build": "webpack ./react --config ./react/webpack.config.js --env.NODE_ENV=production",
    "react:gc": "node -r dotenv/config ./scripts/reactComponentGenerator",
    "server:dev": "nodemon --exec 'node -r dotenv/config' ./server/index.js",
    "lint:server": "eslint ./server --fix",
    "lint:scripts": "eslint ./scripts --fix",
    "lint:react": "eslint ./react --fix",
    "test": "jest .",
    "test:watch": "jest . --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/shayan02/flickr-photo-feed-.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "react",
    "redux",
    "angular"
  ],
  "author": "shayan",
  "license": "MIT",
  "homepage": "https://github.com/shayan02/flickr-photo-feed-",
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.7.4",
    "@babel/plugin-transform-react-display-name": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/runtime": "7.7.4",
    "axios": "0.19.0",
    "axios-jsonp": "1.0.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "polished": "3.4.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-content-loader": "^4.3.2",
    "react-dom": "16.12.0",
    "react-native-svg": "^9.13.3",
    "react-redux": "7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spinners": "^0.6.1",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-observable": "1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "6.5.3",
    "styled-components": "4.4.1",
    "styled-reset": "4.0.7",
    "type-to-reducer": "1.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.7.4",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.7.0",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "express": "4.17.1",
    "faker": "4.1.0",
    "file-loader": "^4.3.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "inquirer": "7.0.0",
    "inquirer-test": "2.0.1",
    "jest": "24.9.0",
    "jest-prop-type-error": "1.1.0",
    "morgan": "1.9.1",
    "node-sass": "6.0.1",
    "nodemon": "2.0.1",
    "path": "0.12.7",
    "strip-indent": "3.0.0",
    "supertest": "4.0.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:server && npm run lint:react",
      "pre-push": "npm run test"
    }
  }
}
